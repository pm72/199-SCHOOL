    "def my_reverse(n):\n",
    "  if n > 0:\n",
    "    my_reverse(n - 1)\n",
    "    arr[n-1] = arr[-n]\n",
    "\n",
    "\n",
    "print(\"Enter an array: \")\n",
    "arr = list(map(int, input().split()))\n",
    "n = len(arr)\n",
    "\n",
    "my_reverse(n)\n",
    "\n",
    "arr"
   ]
